<p>new</p>

<span>
    <%= link_to '投稿一覧', '/blogs' %>
</span>
# ここまで追加
<h1>投稿フォーム</h1>
  <%= form_for(@list, url: '/blogs') do |f| %>
  
    <div class='form_group'>
      <h3>タイトル</h3>
      <%= f.text_field :title %>
      
    </div>
    <div class='form_group'>
      <h3>コメント</h3>
      <%= f.text_area :body %>
    </div>
    <div class='form_group'>
      <h3>画像</h3>
      <%= f.file_field :img, class: "image" %>
      <%= f.submit '投稿' %>
    </div>
<% end %>